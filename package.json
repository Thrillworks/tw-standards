{
  "name": "thrillworks-standards",
  "version": "0.1.0",
  "private": true,
  "description": "Thrillworks Standards",
  "scripts": {
    "refresh": "npm run build && npm run develop",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "lint-fix": "eslint ./src --fix --ext ts --ext tsx --ext js --ext jsx"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.12.0",
    "gatsby": "^3.0.1",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-manifest": "^3.0.0",
    "gatsby-plugin-react-helmet": "^4.0.0",
    "gatsby-plugin-styled-components": "^4.0.0",
    "gatsby-plugin-typegen": "^2.2.4",
    "gatsby-plugin-typescript": "^3.0.0",
    "gatsby-remark-prismjs": "^4.0.0",
    "gatsby-source-filesystem": "^3.0.0",
    "gatsby-source-graphql": "^3.0.0",
    "gatsby-transformer-remark": "^3.0.0",
    "prismjs": "^1.23.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-markdown": "^5.0.3",
    "styled-components": "^5.2.1",
    "ts-node": "^9.1.1"
  },
  "devDependencies": {
    "@thrillworksinc/eslint-config-thrillworks": "^1.0.0",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-helmet": "^6.1.0",
    "@types/styled-components": "^5.1.7",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "typescript": "^4.2.3"
  },
  "keywords": [
    "thrillworks"
  ],
  "license": "MIT"
}
